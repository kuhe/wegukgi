.horizontal-gradient-grey {
    background: #e6e7e9; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(left,  #e6e7e9 0%, #edeef0 6%, #edeef0 96%, #e6e7e9 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,#e6e7e9), color-stop(6%,#edeef0), color-stop(96%,#edeef0), color-stop(100%,#e6e7e9)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left,  #e6e7e9 0%,#edeef0 6%,#edeef0 96%,#e6e7e9 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left,  #e6e7e9 0%,#edeef0 6%,#edeef0 96%,#e6e7e9 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left,  #e6e7e9 0%,#edeef0 6%,#edeef0 96%,#e6e7e9 100%); /* IE10+ */
    background: linear-gradient(to right,  #e6e7e9 0%,#edeef0 6%,#edeef0 96%,#e6e7e9 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e6e7e9', endColorstr='#e6e7e9',GradientType=1 ); /* IE6-8 */
}
.vertical-gradient(@c1,@c2){
    background: @c1; /* Old browsers */
    background: -moz-linear-gradient(top,  @c1 83%, @c2 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(83%, @c1), color-stop(100%,@c2)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  @c1 83%,@c2 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  @c1 83%,@c2 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  @c1 83%,@c2 100%); /* IE10+ */
    background: linear-gradient(to bottom,  @c1 83%,@c2 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='@{c1}', endColorstr='@{c2}',GradientType=0 ); /* IE6-8 */
}
.vertical-gradient-grey {
    background: #eeeff1; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  #eeeff1 83%, #e7e7e9 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(83%,#eeeff1), color-stop(100%,#e7e7e9)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  #eeeff1 83%,#e7e7e9 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #eeeff1 83%,#e7e7e9 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #eeeff1 83%,#e7e7e9 100%); /* IE10+ */
    background: linear-gradient(to bottom,  #eeeff1 83%,#e7e7e9 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#eeeff1', endColorstr='#e7e7e9',GradientType=0 ); /* IE6-8 */
}